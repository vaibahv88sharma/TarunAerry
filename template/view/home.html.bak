            <form name="ContactForm" id="ContactForm" novalidate ng-submit="submitForm(contact)" role="form" action="//formspree.io/vic.sharma.it@gmail.com" method="post">
                <div class="form-group">
                    <label for="txtName">Name</label>
                    <input id="txtName" type="text" class="form-control" name="txtName" maxlength="100" placeholder="Name *" required ng-model="contact.fName">
                    <span ng-show="ContactForm.txtName.$invalid && !ContactForm.txtName.$pristine" class="text-danger">Enter Name</span>
                </div>
                <div class="form-group">
                    <label for="email">Email address</label>
                    <input type="email" class="form-control" name="email" maxlength="100" placeholder="Enter Email *" required ng-model="contact.emailComplete">
                    <span ng-show="ContactForm.email.$invalid && !ContactForm.email.$pristine" class="text-danger">Enter email</span>
                </div>
                <div class="form-group">
                    <label for="subject">Subject</label>
                    <input id="subject" type="text" class="form-control" name="subject" maxlength="100" placeholder="Subject *" required ng-model="contact.subject">
                    <span ng-show="ContactForm.subject.$invalid && !ContactForm.subject.$pristine" class="text-danger">Enter Subject</span>
                </div>
                <button type="submit" class="btn btn-default pull-right" value="Submit" ng-disabled="ContactForm.$invalid" >Submit</button>
            </form>
